{
    "version": 2,
    "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "public/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/upload", "dest": "/api/upload" },
      { "src": "/feedback", "dest": "/api/feedback" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }
  